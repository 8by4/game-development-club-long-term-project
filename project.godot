; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Game Dev Club Game"
run/main_scene="uid://dsbvv6hdm54qc"
config/features=PackedStringArray("4.5", "Forward Plus")
run/flush_stdout_on_print=true
boot_splash/image="uid://dxk7uqxqy3l4q"
config/icon="res://icon.svg"

[autoload]

Main="*res://scripts/main.gd"

[debug]

file_logging/log_path="user://game.log"
file_logging/max_log_files=1

[display]

display_server/driver.linuxbsd="wayland"

[editor]

run/main_run_args="--show-fps"

[file_customization]

folder_colors={
"res://assets/": "blue",
"res://entities/": "purple",
"res://scenes/": "red",
"res://scripts/": "yellow",
"res://ui/": "teal"
}

[physics]

common/physics_ticks_per_second=30
common/enable_object_picking=false

[rendering]

textures/canvas_textures/default_texture_filter=0
gl_compatibility/driver.macos="opengl3_angle"
gl_compatibility/nvidia_disable_threaded_optimization=false
renderer/rendering_method="mobile"
shader_compiler/shader_cache/strip_debug=true
